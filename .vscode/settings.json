{
    "typescript.check.workspaceVersion": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "bin": true,
        "obj": true,
        "dist": true,
        "Properties": true,
        ".vs": true,
        "*.csproj": true,
        "*.sln": true,
        "*.csproj.user": true,
        "*.suo": true
    },
    "cSpell.words": [
        "abpzerotemplate",
        "Consts",
        "Daterangepicker",
        "encrpted",
        "Informations",
        "localforage",
        "metronic",
        "Nootify",
        "oidc",
        "searchbox",
        "yatch"
    ],
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    }
}